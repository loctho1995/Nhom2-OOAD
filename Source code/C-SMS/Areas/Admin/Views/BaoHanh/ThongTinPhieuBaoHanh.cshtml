
<div class="col-md-8">
    <div class="panel rounded shadow">
        <div class="panel-heading">
            <div class="pull-left">
                <h3 class="panel-title">Thông tin bảo hành</h3>
            </div>
            <div class="clearfix"></div>
        </div><!-- /.panel-heading -->
        <div class="panel-body no-padding">
            <form class="form-horizontal form-bordered" role="form">
                <div class="form-body">
                    @foreach (var a in ViewBag.phieuBaoHanh)
                    {
                        <div class="x_content">
                            <div class="form-group">
                                <label for="firstname-1" class="col-sm-4 control-label"> Model name</label>
                                <div class="col-sm-8">
                                    <input value="@a.modelName" type="text" class="form-control rounded" disabled="disabled" />
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="password-1" class="col-sm-4 control-label"> Ngày nhận máy</label>
                                <div class="col-sm-8">
                                    <input value="@a.ngayLap.ToString("dd/MM/yyyy")" disabled="disabled" type="text" class="form-control rounded" />
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="password-1" class="col-sm-4 control-label"> Ngày trả máy</label>
                                <div class="col-sm-8">
                                    <input value="@a.ngayGiao.ToString("dd/MM/yyyy")" disabled="disabled" type="text" class="form-control rounded" />
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </form>
        </div>
    </div>
</div>

<div class="col-md-4">
    <div class="panel rounded shadow">
        <div class="panel-heading">
            <div class="pull-left">
                <h3 class="panel-title">Thông tin phiếu bảo hành</h3>
            </div>
            <div class="clearfix"></div>
        </div><!-- /.panel-heading -->
        <div class="panel-body no-padding">
            <form class="form-horizontal form-bordered" role="form">
                <div class="form-body">
                    @foreach (var a in ViewBag.phieuBaoHanh)
                    {
                        <div class="x_content">
                            <div class="form-group">
                                <label for="firstname-1" class="col-sm-4 control-label">Số phiếu</label>
                                <div class="col-sm-8">
                                    <input value="@a.soPhieuBaoHanh" type="text" class="form-control rounded" disabled="disabled" />
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="password-1" class="col-sm-4 control-label">Ngày lập</label>
                                <div class="col-sm-8">
                                    <input value="@a.ngayLap.ToString("dd/MM/yyyy")" disabled="disabled" type="text" class="form-control rounded" />
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="firstname-1" class="col-sm-4 control-label">Khách hàng</label>
                                <div class="col-sm-8">
                                    <input value="@a.tenKhachHang" type="text" class="form-control rounded" disabled="disabled" />
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="firstname-1" class="col-sm-4 control-label">Số điện thoại</label>
                                <div class="col-sm-8">
                                    <input value="@a.soDienThoai" type="text" class="form-control rounded" disabled="disabled" />
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="firstname-1" class="col-sm-4 control-label">Ghi chú</label>
                                <div class="col-sm-8">
                                    <textarea placeholder="Ghi chú viết tại đây" class="form-control rounded" disabled="disabled">@a.ghiChu</textarea>
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </form>
        </div>
    </div>
</div>

<script src="~/Scripts/bootstrap.min.js"></script>