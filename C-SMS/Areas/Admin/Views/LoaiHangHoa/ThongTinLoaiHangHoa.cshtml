@model IEnumerable<Common.Models.LoaiHangHoa>

@foreach (var item in Model)
{
    <div class="form-group">
        <label class="control-label">Mã Loại Hàng Hóa</label>
        <input type="text" name="maloaihanghoa" value="@item.MaLoaiHangHoa" class="form-control rounded" disabled="disabled" />
    </div>


    <div class="form-group">
        <label class="control-label">Tên Loại Hàng Hóa</label>
        <input type="text" name="tenloaihanghoa" value="@item.TenLoaiHangHoa" class="form-control rounded" required="required" />
    </div>

    <div class="form-group">
        <label class="control-label">Phần Trăm Lợi Nhuận (%)</label>
        <input type="text" name="phantramloinhuan" value="@item.PhanTramLoiNhuan" class="form-control rounded" required="required"/>
    </div>
}

