@model IEnumerable<Common.ViewModels.KiemKhoViewModel>

@{
    ViewBag.Title = "Thông Tin Chi Tiết Phiếu Bán Hàng";
    Layout = "~/Areas/Admin/Views/Layout/HomeLayout.cshtml";
}

<link href="~/Content/Css/Webgird.css" rel="stylesheet" />

<div class="header">
    <div class="col-sm-5 no-padding-left">
        <h2>@ViewBag.Title</h2>
    </div>
    <div class="pull-right">
        <button class="btn btn-dark" onclick="@("window.location.href='" + @Url.Action("Index", "BanHang") + "'"); ">
            <i class="glyphicon glyphicon-arrow-left"></i> Quay lại
        </button>
    </div>
</div>

<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="row">
                @{Html.RenderAction("ThongTinPhieuBanHang", "BanHang");}
            </div>
        </div>
    </div>
</div>

